<form action="" method="post" autocomplete="off">
  <div class="mb-3">
    <label class="form-label" for="nombre">Nombre del bien</label>
    <input type="text" id="nombre" class="form-control" id="nombre"
      placeholder="Computadoras, Sillas, Mesas, etc...">
  </div>
  <div class="row mb-3">
    <div class="col-md-6">
      <label class="form-label" for="fecha_ingreso">Fecha de ingreso</label>
      <input type="date" id="fecha_ingreso" class="form-control" id="fecha_ingreso"
        placeholder="Computadoras, Sillas, Mesas, etc...">
    </div>
    <div class="col-md-6">
      <label class="form-label" for="categorias_id">Categoria</label>
      <select id="categoria_id" name="categorias_id" id="categorias_id" class="form-select">
        <option disabled selected hidden>Seleccione la categoria</option>
      </select>
    </div>
  </div>
  <div class="mb-3">
    <label class="form-label" for="trabajadores_id">Trabajador encargado</label>
    <select id="trabajadores_id" name="trabajadores_id" id="trabajadores_id" class="form-select">
      <option disabled selected hidden>Seleccione el trabajador</option>
    </select>
  </div>
  <div class="mb-3">
    <label class="form-label" for="sedes_id">Ubicacion</label>
    <select id="sedes_id" name="sedes_id" id="sedes_id" class="form-select">
      <option disabled selected hidden>Seleccione la sede</option>
    </select>
  </div>
</form>